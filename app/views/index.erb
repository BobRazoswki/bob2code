
<html>
<head>
	<title>Bookmark Manager</title>
</head>
<body>
  <h1>Homepage</h1>

  <ul class="home__ul">
    <% @links.each do |link| %>
      <li class="home__ul--li">
        <h3><%= link.title %></h3>
        <%= link.url %>
      </li>
    <% end%>
  </ul>

  <form action="/" method="post">
    <label>Title</label>
    <input name="Title" type="text" >
    <label>Url</label>
    <input name="Url" type="text" >
    <input name="Bob It !" type="submit" value="Bob It !">
  </form>

</body>
</html>
